<?xml version="1.0" encoding="gbk" ?>


<!DOCTYPE struts-config PUBLIC
          "-//Apache Software Foundation//DTD Struts Configuration 1.3//EN"
          "http://struts.apache.org/dtds/struts-config_1_3.dtd">

<struts-config>
	<form-beans>
		<form-bean name="dicForm" type="com.zyy.struts.form.DicForm"></form-bean>
	</form-beans>

	<action-mappings>
		<action path="/dic" 
		type="com.zyy.struts.action.DicAction" 
		name="dicForm"
		parameter="action"
		></action>
	</action-mappings>
</struts-config>
