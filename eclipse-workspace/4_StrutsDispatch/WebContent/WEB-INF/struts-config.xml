<?xml version="1.0" encoding="gbk" ?>


<!DOCTYPE struts-config PUBLIC
          "-//Apache Software Foundation//DTD Struts Configuration 1.3//EN"
          "http://struts.apache.org/dtds/struts-config_1_3.dtd">

<struts-config>
	
	<form-beans>
		<form-bean name="CustomerForm" type="com.zyy.struts.form.CustomerForm"></form-bean>
	</form-beans>
	
	<action-mappings>
		<action path="/customer"
				type="com.zyy.struts.action.CustomerAction"
				name="CustomerForm"
				parameter="action"
				>
		</action>
	</action-mappings>
</struts-config>
